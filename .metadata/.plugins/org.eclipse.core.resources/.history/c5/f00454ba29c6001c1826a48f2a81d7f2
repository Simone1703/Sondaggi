package com.example.demo.model;

import java.util.Date;

public class Risposta {
	private int id;
	private Date data;
	private int codRispAmmessa;
	private int autore;
	
	public Risposta(int id, Date data, int codRispAmmessa, int autore) {
		super();
		this.id = id;
		this.data = data;
		this.codRispAmmessa = codRispAmmessa;
		this.autore = autore;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Date getData() {
		return data;
	}

	public void setData(Date data) {
		this.data = data;
	}

	public int getCodRispAmmessa() {
		return codRispAmmessa;
	}

	public void setCodRispAmmessa(int codRispAmmessa) {
		this.codRispAmmessa = codRispAmmessa;
	}

	public int getAutore() {
		return autore;
	}

	public void setAutore(int autore) {
		this.autore = autore;
	}
	
}
